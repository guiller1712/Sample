                              <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
                           </VBox.margin>
                        </JFXComboBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
                     <children>
                        <JFXTextField fx:id="txtEst" promptText="Estatus">
