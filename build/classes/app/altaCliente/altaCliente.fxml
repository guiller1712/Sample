<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="app.altaCliente.AltaClienteController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <bottom>
            <HBox alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="600.0" BorderPane.alignment="CENTER">
